module task1(	address, CLOCK_50, data, wren, q);
	input	[4:0]  address;
	input	  		 CLOCK_50;
	input	[2:0]  data;
	input	  		 wren;
	output[2:0]  q;
	
	assign clock = CLOCK_50;
	
	ram32x3 ram ( .address, .clock, .data, .wren, .q);
endmodule

